{
  "name": "eddie-the-lizard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://westerwynhollow.github.io",
  "dependencies": {
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.4",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.15.0",
    "shopify-buy": "^1.0.0-alpha.9"
  },
  "devDependencies": {
    "react-scripts": "1.0.12"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
